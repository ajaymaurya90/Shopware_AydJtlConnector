{
    "name": "ayd-jtl-connector/ayd-jtl-connector",
    "description": "Fetch JTL data for Shopware PDP",
    "type": "shopware-platform-plugin",
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "shopware/core": "~6.5",
        "symfony/http-client": "^6.3",
        "symfony/cache": "^6.3"
    },
    "extra": {
        "shopware-plugin-class": "AydJtlConnector\\AydJtlConnector",
        "label": {
            "de-DE": "Skeleton plugin",
            "en-GB": "Skeleton plugin"
        }
    },
    "autoload": {
        "psr-4": {
            "AydJtlConnector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AydJtlConnector\\Tests\\": "tests/"
        }
    }
}
